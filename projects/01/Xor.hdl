/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    And(a=a, b=notB, out=aNotBOut);
    And(a=b, b=notA, out=bNotAOut);
    Or(a=aNotBOut, b=bNotAOut, out=out);
}
